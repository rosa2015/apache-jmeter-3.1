          <stringProp name="BeanShellSampler.query">//生成随机生日
import java.math.BigDecimal;
String yearBefore=&quot;19&quot;;
int yearRound=(int)(Math.random()*90+10);//随机产生两位数，10-99之间；Math.random()产生（0，1）之间数字
String year=yearBefore+yearRound.toString();
int monthRound=(int)(Math.random()*12+1);
int dayRound=(int)(Math.random()*30+1);
public class getTwoNum{
     public String Num(int b){
	if(b&lt;10){
                    String str=&quot;0&quot;+b.toString();
	     return str;
	}
            else{
	    String str=b.toString();
	    return str;
	}
}
}
getTwoNum gtn=new getTwoNum();
String birthday=year+&quot;-&quot;+gtn.Num(monthRound)+&quot;-&quot;+gtn.Num(dayRound);
vars.put(&quot;birthday&quot;,birthday);
//性别，实名认证、国家码
String[] str_sex={&quot;MALE&quot;,&quot;FEMALE&quot;,&quot;NON-BINARY&quot;};//性别,MALE=男性,FEMALE=女性,NON-BINARY=其他
String[] str_auth={&quot;UNREAL&quot;,&quot;SUCCESS&quot;,&quot;FAILURE&quot;}; //实名状态,UNREAL=未实名,SUCCESS=实名认证成功,FAILURE=实名认证失败
int random=(int)(Math.random()*3);
int random2=(int)(Math.random()*20+1);//获取随机客户id使用
vars.put(&quot;sex&quot;,str_sex[random]);
vars.put(&quot;real_name_status&quot;,str_auth[random]);
vars.put(&quot;num_random&quot;,random2.toString());
</stringProp>
